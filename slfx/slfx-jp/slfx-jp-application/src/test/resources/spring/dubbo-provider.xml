<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans        http://www.springframework.org/schema/beans/spring-beans.xsd        http://code.alibabatech.com/schema/dubbo        http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

	<dubbo:application name="slfx-jp-provider-dev" />

	<dubbo:registry address="${dubbo_register_center}"/>

	<dubbo:protocol name="dubbo" port="20881" />

	<dubbo:service interface="slfx.jp.spi.inter.FlightStopService" ref="jpFlightStopService" />

	<dubbo:service interface="slfx.jp.spi.inter.PlatformAskOrderTicketService" ref="jpPlatformAskOrderTicketService" />
	
	<dubbo:service interface="slfx.jp.spi.inter.JPPlatformOrderService" ref="jpPlatformOrderService" />
	
	<dubbo:service interface="slfx.jp.spi.inter.FlightPolicyService" ref="flightPolicyService" />
	
	<dubbo:service interface="slfx.jp.spi.inter.WebFlightService" ref="webFlightService" />
	
	<dubbo:service interface="slfx.jp.spi.inter.JPPlatformApplyRefundService" ref="jpPlatformApplyRefundService" />

	<dubbo:service interface="slfx.jp.spi.inter.PlatformGetRefundActionTypeService" ref="jpPlatformGetRefundActionTypeService" />

</beans>