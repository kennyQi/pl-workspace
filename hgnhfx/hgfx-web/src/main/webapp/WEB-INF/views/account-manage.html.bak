<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>账号管理</title>
	<link  href="css/common.css" type="text/css" rel=stylesheet>
	<link  href="css/common-b.css" type="text/css" rel=stylesheet>
    <link  href="css/order.css" type="text/css" rel=stylesheet>
<script src="js/jquery-1.12.3.min.js" charset="utf-8"></script>	
<script src="js/WdatePicker.js" charset="utf-8"></script>
<script src="js/common.js" charset="utf-8"></script>
<script src="js/left.js" charset="utf-8"></script>
</head>
<style>

</style>
<script>
	function leftClick(el){
		alert(el.html());
	}

	$(function(){
		
		var a=$(".table-content-title").find(".time");
		var b=$(".table-content-title").find(".amount");
		a.click(function(){
			$(this).addClass("active");
			b.removeClass("active");
			
			$(this).find("img").hide();
			b.find("img").show();
		});
		
		b.click(function(){
			$(this).addClass("active");
			a.removeClass("active");
			
			$(this).find("img").hide();
			a.find("img").show();
		});
		
	});
</script>
<body>
	<div class="mininav">
		<div class="banner">
			<div class="banner-pure">
				<div class="logo"></div>
				<div class="logo-gap"></div>
				<div class="logo2"></div>
				
				<div class="banner-right">
					<img src="img/order/icon.png">
					<span class="user">admin</span>
					<a href="javascript:void(0);"><span class="out">退出</span></a>
				</div>
			</div>
		</div>
		<div id="content">
			<div class="content-wrap">
				<div class="content-wrap-pure">
