<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="hg.demo.member.service.dao.mybatis.ProductInUseMyBatisDao">

     <resultMap id="productUseMap" type="hg.fx.domain.Product">
     <result property="id" column="ID" />
      <result property="prodName" column="PROD_NAME" />
      <result property="prodCode" column="PROD_CODE" />
     </resultMap>
	<select id="productNotUseList" parameterType="String" resultMap="productUseMap">
	 select 
	 hp.ID,
	 hp.PROD_NAME,
	 hp.PROD_CODE
	from hgfx_product hp 
	where hp.ID not in(
	  select distinct u.prod_id
	  from hgfx_product_in_use u
	  where u.distributor_id = #{distributorId}
	)
	
	
	</select>

</mapper>