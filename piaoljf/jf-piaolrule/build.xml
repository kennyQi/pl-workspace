<?xml version="1.0" encoding="GBK" standalone="no"?>
<project default="dist" name="Create Runnable Jar for Project ${projectname}_1">
    <!--this file was created by Eclipse Runnable JAR Export Wizard-->
    <!--ANT 1.7 is required                 
                           -->
	<!-- 请修改下面2处，分别为模版类名和中文名 -->
	<property name="logic" value="QQTXDiscountLogic"></property>
	<property name="name" value="权倾天下优惠券模版"></property>
	
	<!--  下面为打包的实现过程 -->
	<target name="dist">
		<echo message="${name}" append="false" file="./README"></echo>
		<jar destfile="${logic}.jar" filesetmanifest="mergewithoutmain">
            <manifest>
                <attribute name="Class-Path" value="."/>
            </manifest>
            <fileset dir="target/classes" includes="**/${logic}.class"/>
            <fileset dir="src/main/java" includes="**/${logic}.java"></fileset>
			<fileset file="./README"></fileset>
        </jar>
				
	</target>
</project>
